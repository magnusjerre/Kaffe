form.centerContainer(action="registrerKarakter", method="POST")
	input(name="sammendrag", type="hidden", required)
	div.v2innhold
		label Kaffe:#[span.requiredNotOkay *]
		select(name="kaffeid")
			option(selected="selected", disabled="disabled") Velg kaffe
	div.v2innhold
		label Bruker:#[span.requiredNotOkay *]
		input.v2innholdInput(name="bruker", placeholder="Jerre", required)
	div.v2innhold
		label Karakter:#[span.requiredNotOkay *]
		include ../karakterskala.jade
	div.v2innhold
		label Kommentar:
		input.v2innholdInput(name="kommentar", placeholder="Denne var god...")
button.bryggsubmit(type="button") Registrer