doctype html
html
	head
		meta(charset="UTF-8")
		script(src="jquery-2.1.4.min.js", type="text/javascript")
		script(src="jQueryRotate.js", type="text/javascript")
		script(src="listdagenskaffe.js", type="text/javascript")
		script(src="index.js", type="text/javascript")
		script(src="kalendervisning.js" type="text/javascript")
		link(rel="stylesheet", type="text/css", href="kaffe.css")
	body.container
		h1 Den store kaffesiden
		include nav.jade
		h2 Desember
		table#kaffekalender.kalender
			tr
				th Mandag
				th Tirsdag
				th Onsdag
				th Torsdag
				th Fredag
				th Lørdag
				th Søndag
			- var n = 0
			while n < 6
				tr
					- var i = n * 7
					- var max = n * 7 + 7
					while i < max
						td 
							p #{dager[i]}
							if dagensbrygg[i] != null
								p(id=i) #{dagensbrygg[i].kaffe_navn}
						- var bogus1 = i++
				- var bogus2 = n++
		include dagenskaffeboks.jade